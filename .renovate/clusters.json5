{
  $schema: "https://docs.renovatebot.com/renovate-schema.json",
  packageRules: [
    {
      description: "Separate PRs for darkstar cluster",
      matchFileNames: ["**/darkstar/**"],
      additionalBranchPrefix: "darkstar-",
    },
    {
      description: "Separate PRs for apollo (storage)",
      matchFileNames: ["**/apollo/**"],
      additionalBranchPrefix: "apollo-",
    },
  ],
}
